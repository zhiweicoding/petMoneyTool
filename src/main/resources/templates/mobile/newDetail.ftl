    function startVideo(url) {
        window.location.href = '${base}/webMobile/good/video?listUrl=' + url
    }

    function buyIt() {
        $.alert("建议使用小程序预定猫咪，微信、支付宝搜索优宠熊即可");
    }

    function likeTap(goodId) {
        $.showLoading()
        $.post('${base}/webMobile/good/addCollect', {
            goodId: goodId
        }, function (data) {
            $.hideLoading()
            if (data.msgCode == 10000) {
                $.toast('已添加我的喜欢')
            } else {
                window.location.href = '${base}/webMobile/mobileLoginPage/login'
            }
        })
    }

    function goHome() {
        window.location.href = '${base}/'
    }

    function openUrl(url) {
        window.location.href = url
    }

    function callPhone() {
        window.location.href = "tel:18302284459";
    }

    function copy(str) {
        var save = function (e) {
            e.clipboardData.setData('text/plain', str);//下面会说到clipboardData对象
            e.preventDefault();//阻止默认行为
        }
        document.addEventListener('copy', save);
        document.execCommand("copy");//使文档处于可编辑状态，否则无效

        $.toast("微信号已复制到剪切板，可直接到微信中粘贴添加即可", "text");

    }
</script>
